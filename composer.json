{
    "name": "zeroplex/blog-crawler",
    "type": "project",
    "description": "crawler for blog",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "johnroyer/crawler-php": "^0.2"
    },
    "require-dev": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "Crawler\\": "src/"
        }
    },
    "autoload-dev": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
